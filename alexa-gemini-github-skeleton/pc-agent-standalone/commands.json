{
  "patterns": [
    {
      "match": "abrir youtube",
      "action": {
        "type": "open_url",
        "url": "https://youtube.com"
      }
    },
    {
      "match": "abrir (https?://\\\\S+)",
      "action": {
        "type": "open_url",
        "url": "{1}"
      }
    },
    {
      "match": "abrir navegador",
      "action": {
        "type": "open_url",
        "url": "https://google.com"
      }
    },
    {
      "match": "abrir calculadora",
      "action": {
        "type": "open_app",
        "windows": "calc.exe",
        "darwin": "open -a Calculator",
        "linux": "gnome-calculator"
      }
    }
  ]
}